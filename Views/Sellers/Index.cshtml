
@model IEnumerable<SellersWebMVC.Models.Seller>

@{
    ViewData["Title"] = "Index";
}

<h2>@ViewData["Title"]</h2>

<p>Sellers // vendedores // teste</p>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.birthDate)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.BaseSalary)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayNameFor(modelItem => item.Name)
            </td>


            <td>
                @Html.DisplayNameFor(modelItem => item.Email)
            </td>


            <td>
                @Html.DisplayNameFor(modelItem => item.birthDate)
            </td>


            <td>
                @Html.DisplayNameFor(modelItem => item.BaseSalary)
            </td>
        </tr>
        }
    </tbody>

</table>


